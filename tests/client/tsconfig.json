{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "removeComments": true,
        "traceResolution": false,
        "module": "amd",
        "moduleResolution": "node",
        "outDir": "../../tests-run/client/",
        "rootDir": "./",
        "target": "es5",
        "baseUrl": "./",
        "paths": {
            "*": [
                "../../node_modules/@types/node/",
                "../../node_modules/typescript/"
            ],
            "tsl-apple-cloudkit": [
                "../../src/tsl-apple-cloudkit"
            ],
            "testsconfig": [
                "./testsconfig"
            ]
        }
    },
    "exclude": [
        "../../node_modules/",
        "../../src/"
    ],
    "files": [
        "./tests.ts"
    ]
}
